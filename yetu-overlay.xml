<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote
		name	= "yetu"
		fetch	= "ssh://git@bitbucket.org/yetu" />

	<remote
		name	= "dereulenspiegel"
		fetch	= "https://github.com/dereulenspiegel" />

	<project
		path	= "src/third_party/tableknife"
		name	= "tableknife.git"
		revision = "master"
		remote	= "dereulenspiegel" />

	<project
		path	= "src/third_party/yetu-tools"
		name 	= "chromium-yetu-tools.git"
		revision = "master"
		remote	= "yetu" />
	
	<project
		path	= "src/third_party/yetu-overlay"
		name	= "chromiumos-yetu-overlay.git"
		remote	= "yetu" />

	<remove-project name="chromiumos/overlays/chromiumos-overlay" />
	<project 
		name	= "chromiumos-chromiumos-overlay.git"
		path	= "src/third_party/chromiumos-overlay"
		remote	= "yetu" />

	<remove-project name="chromiumos/overlays/board-overlays" />
	<project
		name = "chromiumos-overlays.git"
		path = "src/overlays"
		remote = "yetu" />

	<remove-project name="chromiumos/platform/crosutils" />
	<project
		name = "chromiumos-scripts.git"
		path = "src/scripts"
		remote = "yetu" />

	<remove-project name="chromiumos/platform2" />
	<project
		name = "chromiumos-platform2.git"
		path = "src/platform2"
		remote = "yetu" />

</manifest>

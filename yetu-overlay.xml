<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote
		name	= "origin"
		fetch	= "ssh://git@bitbucket.org/yetu" />

	<remote
		name	= "yetu"
		fetch	= "ssh://git@bitbucket.org/yetu" />
	
	<project
		path	= "src/overlays/overlay-yetuamd64"
		name	= "chromiumos-overlay-yetu-amd64.git"
		remote	= "origin" />

	<remove-project name="chromiumos/overlays/chromiumos-overlay" />
	<project 
		name	= "chromiumos-chromiumos-overlay.git"
		path	= "src/third_party/chromiumos-overlay"
		revision = "jdk"
		remote	= "yetu" />

	<remove-project name="chromiumos/overlays/portage-stable" />
	<project
		name	= "chromiumos-portage-stable.git"
		path	= "src/third_party/portage-stable"
		revision = "jdk7"
		remote	= "yetu" />
</manifest>
